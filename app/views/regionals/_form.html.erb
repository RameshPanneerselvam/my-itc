<div class="row">
                 <div class="col-md-3"></div><div class="col-md-6">
               <div class="panel panel-primary">
                        <div class="panel-heading">
                          New Registration
                           </div>
                        <div class="panel-body"><%= form_for(regional) do |f| %>
  <% if regional.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(regional.errors.count, "error") %> prohibited this regional from being saved:</h2>

      <ul>
      <% regional.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <div class="form-group">
  
    <%= f.label :regional_name %>
    <%= f.text_field :regional_name ,:class=>"form-control", :required=>"true"%>
  </div>
    <div class="form-group">

    <%= f.label :regional_code %>
    <%= f.text_field :regional_code ,:class=>"form-control", :required=>"true"%>
  </div>

  <%= f.fields_for :contact_attributes do |ff| %>
  <div class="form-group">
    <%= ff.label :landline %>
    <%= ff.text_field :landline ,:class=>"form-control",:required=>"true"%>
  </div>
 
   <div class="form-group">
    <%= ff.label :extension %>
    <%= ff.text_field :extension,:class=>"form-control",:required=>"true"%>
  </div>
 <div class="form-group">
    <%= ff.label :mobilenumber1 %>
    <%= ff.text_field :mobilenumber1,:class=>"form-control" ,:required=>"true"%>
  </div>
 <div class="form-group">
    <%= ff.label :mobilenumber2  %>
    <%= ff.text_field :mobilenumber2,:class=>"form-control",:required=>"true"%>
  </div>
 <div class="form-group">
    <%= ff.label :email %>
    <%= ff.text_field :email,:class=>"form-control",:required=>"true" %>
  </div>
<% end %>

<%= f.fields_for :address_attributes do |ff| %>
<div class="form-group">
    <%= ff.label :address1 %>
    <%= ff.text_field :address1 ,:class=>"form-control",:required=>"true"%>
  </div>
 <div class="form-group">
    <%= ff.label :address2 %>
    <%= ff.text_field :address2,:class=>"form-control"%>
  </div>
<div class="form-group">
    <%= ff.label :country %>
  <select name="regional[address_attributes[country]]" class="countries form-control" id="countryId">
<option value="">Select Country</option>
</select>
  </div>
 <div class="form-group">
    <%= ff.label :state %>
    <select name="regional[address_attributes[state]]" class="states form-control" id="stateId">
<option value="">Select State</option>
</select>
  </div>
<div class="form-group">
    <%= ff.label :city %>
<select name="regional[address_attributes[city]]" class="cities form-control" id="cityId">
<option value="">Select City</option>
</select>
  </div>
<% end %>

  <div class="form-group">
    <%= f.submit 'create',:class=>"btn btn-primary" %>
  </div>
<% end %>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
<script src="http://lab.iamrohit.in/js/location.js"></script>

